<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="DeptUseTemplate">
    <page>/untitled1.jsf</page>
  </view>
  <view id="EmpUseTemplate">
    <page>/EmpUseTemplate.jsf</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__2">
      <from-outcome>GoToDeptTemp</from-outcome>
      <to-activity-id>DeptUseTemplate</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__3">
      <from-outcome>GoToEmpTemp</from-outcome>
      <to-activity-id>EmpUseTemplate</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>
