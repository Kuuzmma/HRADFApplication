<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="task-flow-definition-train">
    <default-activity>EmpTrain</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="EmpTrain">
      <page>/EmpTrain.jsff</page>
      <train-stop>
        <display-name>Employees</display-name>
      </train-stop>
    </view>
    <view id="DeptTrain">
      <page>/DeptTrain.jsff</page>
      <train-stop>
        <display-name>Department</display-name>
      </train-stop>
    </view>
    <train/>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
